{
  "private": true,
  "name": "z1-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --template src/template.html --no-service-worker",
    "serve": "preact build && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-jest": "^23.6.0",
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "mockdate": "^2.0.5",
    "node-sass": "^7.0.0",
    "per-env": "^1.0.2",
    "preact-cli": "^2.1.0",
    "preact-cli-plugin-fast-async": "^1.0.1",
    "preact-cli-sw-precache": "^1.0.3",
    "preact-render-spy": "^1.2.1",
    "sass-loader": "^7.1.0",
    "workbox-webpack-plugin": "^4.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@sentry/browser": "^5.10.2",
    "common": "https://github.com/z1-team/common.git",
    "copy-webpack-plugin": "^5.0.2",
    "detect-browser": "^4.0.3",
    "path": "^0.12.7",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-context": "^1.1.3",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.5.7",
    "react-masonry-component": "^6.2.1",
    "react-sticky-box": "https://github.com/vladimir-shalaev22/react-sticky-box.git",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sticky-sidebar": "^3.3.1",
    "uuid-js": "^0.7.5"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js"
    ],
    "testRegex": "(/(__tests__|tests)/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/tests/__mocks__/*"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "^create-react-class$": "preact-compat/lib/create-react-class",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  }
}
